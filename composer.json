{
    "name": "rvdlee/doctrine-data-fixture-module",
    "description": "A port for ZF3 of Hounddog's original data-fixture code with the ability to change the executor for fixtures.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "data-fixture",
        "module",
        "zf2"
    ],
    "homepage": "http://www.doctrine-project.org/",
    "authors": [
        {
            "name": "Martin Shwalbe",
            "email": "martin.shwalbe@gmail.com"
        },
        {
            "name": "Rob van der Lee",
            "email": "r.vdlee@salesupply.com"
        }
    ],
    "require": {
        "php": "^7",
        "doctrine/data-fixtures": "^1.3",
        "doctrine/doctrine-orm-module": "^1.1",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "require-dev": {
        "zendframework/zendframework": "3.*",
        "symfony/symfony": "^3.4",
        "phpunit/phpunit" : "^6",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-0": {
            "DoctrineDataFixtureModule": "src/"
        }
    }
}
